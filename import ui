1_2 = cell(1,2)
1_3 = cell(1,3)
1_4 = cell(1,4)
1_5 = cell(1,5)

2_1 = cell(2,1)
2_2 = cell(2,2)
2_2.cell.value = 1
2_3 = cell(2,3)
2_4 = cell(2,4)
2_5 = cell(2,5)

for x in gridFrame[y]:
	print(x.cell.value)

