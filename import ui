import ui

class cell:
	self.value = 0.5
	def __init__(self, x, y):
		self.x = x
		self.y = y
		self.position = (x, y)

grid = ui.load_view('grid_view')

gridFrame = [[1-1,1-2,1-3,1-4,1-5],[2-1,2-2,2-3,2-4,2-5]]

